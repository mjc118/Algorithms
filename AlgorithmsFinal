#include <iostream>
#include <fstream>
#include <algorithm>
#include <string>

using namespace std;

int main(){
	ifstream input("en_US.dic");
	ofstream output("en_US_processed.dic");
	string line;
	int l = 0;
	while (getline(input, line)){
		cout << ++l << endl;
		if (count(line.begin(), line.end(), '/')){
			line = line.substr(0, line.find("/"));
		}
		output << line << endl;
	}
	output.close();
	input.close();


	return 0;
}
